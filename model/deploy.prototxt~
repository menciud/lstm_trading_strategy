layer {
  name: "LSTM1"
  type: "LSTM"
  bottom: "Reshape1"
  bottom: "Reshape2"
  top: "LSTM1"
  recurrent_param {
    num_output: 32
    weight_filler {
      type: "uniform"
      min: -0.01
      max: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "InnerProduct3"
